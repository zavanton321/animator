<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.animator"
    >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        >
        <activity android:name=".type_evaluator.TypeEvaluatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".animated_state_list_drawable.AnimatedStateListDrawableActivity" />
        <activity android:name=".state_list_animator.LoadStateListAnimatorActivity" />
        <activity android:name=".state_list_animator.StateListAnimatorActivity" />
        <activity android:name=".listeners.ListenersActivity" />
        <activity android:name=".animators.AnimatorActivity" />
    </application>

</manifest>